@model IEnumerable<RestaurantSystem.DataModel.Ingredient<int>>
@{
    Layout = null;
}
<!doctype html>
<html>
<head>
    <title>All dish types</title>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width">
</head>
<body>
<div>
    <a href="/Dishes">back</a>
</div>
<table>
    @foreach (var ingredient in Model)
    {
        <tr>
            <td>@ingredient.Weight</td>
            <td>@ingredient.Decription</td>
        </tr>
    }
</table>
</body>
</html>